#include <stdio.h>
#include <string.h>
int main()
{
    char text[]="example";
    char pattern[]="am";
    int n=strlen(text);
    int m=strlen(pattern);
    int matches=0;
    for (int i=0;i<=n-m;i++) 
    {
        int j=0;
        while (j<m && text[i+j]==pattern[j]) 
          j++;
        if (j==m) 
          matches++;
    }
    printf("Number of matches found: %d\n", matches);
    return 0;
}




