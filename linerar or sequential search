#include <stdio.h>
int main() {
    int i, n, k;
    int a[] = {1, 2, 3, 4, 5, 6, 7};
    n = sizeof(a) / sizeof(a[0]);
    k = 3;
    i = 0;
    while (i < n && a[i] != k)
    {
        i = i + 1;
    }
    if (i < n) 
    {
        printf("Element %d found at index %d\n", k, i);
        return 0; 
    }
    else 
    {
        printf("Element %d not found\n", k);
        return 1; 
    }
}
