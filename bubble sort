#include <stdio.h>
int main()
{
    int a[] = {30,10,20};
    int n=sizeof(a)/sizeof(a[0]);
    int i,j;
    for (i=0;i<=n-2;i++) 
    {
        for (j=0;j<=n-i-2;j++) 
        {
            if (a[j+1]<a[j])
            {
                a[j]=a[j]+a[j+1];
                a[j+1]=a[j]-a[j+1];
                a[j] =a[j]-a[j+1];
            }
        }
    }
    printf("Sorted array: \n");
    for (i=0;i<n;i++) 
    {
        printf("%d ", a[i]);
    }
  }
